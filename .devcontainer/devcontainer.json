{
  "name": "Optimized TransportMat Dev",
  "image": "mcr.microsoft.com/devcontainers/javascript-node:18-alpine",
  "postCreateCommand": "npm ci",
  "customizations": {
    "vscode": {
      "settings": {
        "editor.minimap.enabled": false,
        "files.watcherExclude": {
          "**/node_modules/**": true,
          "**/.git/**": true
        }
      },
      "extensions": [
        "GitHub.copilot",
        "GitHub.copilot-chat",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "kodu-ai.claude-dev-experimental",
        "killokoder.killokoder-vscode"
      ]
    }
  }
}
