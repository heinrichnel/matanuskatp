{
  "name": "vite-react-typescript-starter",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc --noEmit --skipLibCheck && vite build",
    "build:debug": "tsc --noEmit && vite build",
    "build:force": "tsc --noEmit --skipLibCheck && vite build",
    "build:production": "VITE_ENV_MODE=production npm run build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "./deploy.sh trigger",
    "deploy:local": "./deploy.sh local",
    "deploy:full": "./deploy.sh both",
    "netlify:env": "echo 'Copy variables from .env.production to Netlify environment settings'"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@radix-ui/react-tabs": "^1.1.12",
    "@react-google-maps/api": "^2.20.7",
    "@react-pdf/renderer": "^4.3.0",
    "@types/d3": "^7.4.3",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.525.0",
    "moment": "^2.30.1",
    "openai": "^5.8.3",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-calendar-timeline": "^0.30.0-beta.3",
    "react-dom": "^19.1.0",
    "react-google-recaptcha-v3": "^1.11.0",
    "react-hook-form": "^7.60.0",
    "react-router-dom": "^7.6.3",
    "react-to-print": "^3.1.1",
