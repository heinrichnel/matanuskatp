{
  "database": {
    "rules": "database.rules.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint",
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "emulators": {
    "functions": {
      "host": "127.0.0.1",
      "port": 5001
    },
    "firestore": {
      "host": "127.0.0.1",
      "port": 8081
    },
    "database": {
      "host": "127.0.0.1",
      "port": 9001
    },
    "storage": {
      "host": "127.0.0.1",
      "port": 9198
    },
    "dataconnect": {
      "port": 9399,
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    },
    "ui": {
      "host": "127.0.0.1",
      "port": 4001
    },
    "singleProjectMode": true,
    "tasks": {
      "port": 9499
    },
    "pubsub": {
      "port": 8085
    }
  },
  "dataconnect": {
    "source": "dataconnect"
  },
  "extensions": {}
}
