{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json",
    "database": "(default)",
    "location": "nam5"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint"
      ]
    }
  ],
  "emulators": {
    "firestore": {
      "host": "127.0.0.1",
      "port": 8081
    },
    "storage": {
      "host": "127.0.0.1",
      "port": 9198
    },
    "functions": {
      "host": "127.0.0.1",
      "port": 8888
    },
    "ui": {
      "host": "127.0.0.1",
      "port": 4001
    },
    "logging": {
      "host": "127.0.0.1",
      "port": 4500
    },
    "hub": {
      "host": "127.0.0.1",
      "port": 4400
    },
    "pubsub": {
      "port": 8085
    },
    "eventarc": {
      "port": 9299
    },
    "singleProjectMode": true
  },
  "remoteconfig": {
    "template": "remoteconfig.template.json"
  },
  "extensions": {}
}